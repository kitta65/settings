{
  "coc.source.around.firstMatch": false,
  "coc.source.buffer.firstMatch": false,
  "snippets": {
    "loadFromExtensions": false
  },
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"]
    },
    "sql": {
      "command": "bq-language-server",
      "args": ["--stdio"],
      "filetypes": ["sql", "bq"]
    }
  },
  // https://github.com/rust-analyzer/rust-analyzer/issues/6905#issuecomment-746841097
  "diagnostic.messageDelay": 200
}
