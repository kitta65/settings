{
  "coc.source.around.firstMatch": false,
  "coc.source.buffer.firstMatch": false,
  "snippets": {
    "loadFromExtensions": false
  },
  "python.jediEnabled": false,
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"]
    },
    "sql": {
      "command": "bq-language-server",
      "args": ["--stdio"],
      "filetypes": ["sql", "bq"]
    }
  }
}
